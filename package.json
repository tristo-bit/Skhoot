{
  "name": "skhoot---your-desktop-seeker",
  "private": true,
  "version": "0.1.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:no-open": "vite --host --open false",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:full": "concurrently \"npm run backend:dev\" \"npm run tauri:dev\"",
    "tauri:build:ubuntu": "tauri build --target x86_64-unknown-linux-gnu",
    "backend:dev": "cd backend && cargo run --bin skhoot-backend",
    "backend:build": "cd backend && cargo build --release",
    "backend:test": "cd backend && cargo test",
    "dev:full": "concurrently \"npm run backend:dev\" \"npm run dev\"",
    "release": "bash scripts/release.sh",
    "release:linux": "bash scripts/release.sh linux",
    "release:macos": "bash scripts/release.sh macos",
    "release:windows": "bash scripts/release.sh windows",
    "release:clean": "bash scripts/release.sh all --clean",
    "prepare:whisper": "node scripts/prepare-whispercpp.js"
  },
  "dependencies": {
    "@google/genai": "^1.35.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-dialog": "^2.0.0",
    "@tauri-apps/plugin-notification": "^2.3.3",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "@types/three": "^0.182.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tauri-apps/cli": "^2.9.6",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.23",
    "concurrently": "^8.2.2",
    "fast-check": "^4.5.3",
    "jsdom": "^23.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^1.0.0"
  }
}
